  <div class="store">

    <% if notice %>
      <p id="notice"><%= notice %></p>
    <% end %>

    <br>
    <h1>Produce In Season Now</h1>


    <% cache ['store', Product.latest] do %>
      <% @products.each do |product| %>
        <% cache ['entry', product] do %>
          <div class="entry">
            <%= image_tag(product.image) %>
            <h3><%= product.title %></h3>
            <h5><%= product.description %></h5>

          </div>
        <% end %>
      <% end %>
    <% end %>

  </div>



